{
    "name": "deai-subnet-dashboard",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 127.0.0.1 --port 8000",
        "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
        "build": "vite build",
        "preview": "vite preview",
        "backend:install": "cd backend && pip install -r requirements.txt"
    },
    "dependencies": {
        "@rainbow-me/rainbowkit": "^2.1.2",
        "chart.js": "^4.5.1",
        "lucide-react": "^0.294.0",
        "react": "^18.2.0",
        "react-chartjs-2": "^5.3.1",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.0",
        "viem": "^2.1.0",
        "wagmi": "^2.6.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.2",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.3.6",
        "vite": "^5.0.8"
    }
}
